@model IEnumerable<MayorAssistantApp.Models.ChatBotModels.QA>

@{
    ViewBag.Title = "Index";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QAText)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QAText)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.QAId })
            @Html.ActionLink("Create", "Create", new { id = item.QAId })
        </td>
    </tr>
}

</table>
